---
const sessionCookie = Astro.cookies.get("session");
if (sessionCookie?.value !== "unsecure_session") {
  return Astro.redirect("/admin/login");
}
---

<main>
  <nav>
    <form method="POST" action="/admin/logout">
      <button type="submit">Logout</button>
    </form>
  </nav>
  <slot />
</main>
